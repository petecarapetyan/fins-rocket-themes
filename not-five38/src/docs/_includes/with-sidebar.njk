<!doctype html>
  {% block htmlTag %}
    <html lang="{{site.lang}}" dir="{{site.dir}}">
  {% endblock htmlTag %}
  <head>
    {% block headContent %}
      {% include 'partials/head-content.njk' %}
    {% endblock %}
  </head>

<body layout="with-sidebar">
  <aside id="sidenav-open">
      {% block sidebarOverlay %}
        <rocket-drawer id="sidebar">
          <nav slot="content" id="sidebar-nav">
            {% include 'partials/logoLink.njk' %}
            <rocket-navigation>
              {{ collections[section] | rocketNav | rocketNavToHtml({
                listItemClass: "menu-item",
                activeListItemClass: "current",
                activeKey: eleventyNavigation.key
              }) | safe }}
              {% include 'partials/mobile-sidebar-bottom.njk' %}
            </rocket-navigation>
          </nav>
        </rocket-drawer>
      {% endblock sidebarOverlay %}
      
      <a href="#" id="sidenav-close" title="Close Menu" aria-label="Close Menu" onclick="window.history.back()"></a>
    </aside>

    <main>
      <div class="ga-head2-main sales-banner">
        <img src="{{ '_assets/_static/images/statefarm-banner.jpg' | asset | url }}" role="presentation"/>
      </div>

      <div class="head2 ga-head3-main">
        <h2>BellumGerrare</h2>
      </div>

      <div class="header-grid-area">
        {% block header %}
          {% include 'partials/header.njk' %}
        {% endblock header%}
      </div>

      <div class="ga-content-l"></div>
      <div class="rocket-content content-grid-area">
        {% block main %}
          <div class="markdown-body">
            {{ content.html | safe }}
            {% include 'partials/previousNext.njk' %}
          </div>
        {% endblock main %}
      </div>
      <div class="sidebar-grid-area">
      {% block sidebar %}
        <rocket-drawer id="sidebar">
          <nav slot="content" id="sidebar-nav">
            {% include 'partials/logoLink.njk' %}
            <rocket-navigation>
              {{ collections[section] | rocketNav | rocketNavToHtml({
              listItemClass: "menu-item",
              activeListItemClass: "current",
              activeKey: eleventyNavigation.key
          }) | safe }}
              {% include 'partials/mobile-sidebar-bottom.njk' %}
            </rocket-navigation>
          </nav>
        </rocket-drawer>
      {% endblock sidebar %}
      </div>
      <div class="ga-content-r"></div>

      <div class="ga-foot-main">
        {% block footer %}
          {% include 'partials/footer.njk' %}
          {% include 'partials/pageJs.njk' %}
          {% include 'partials/mobileNavigationJs.njk' %}
        {% endblock footer %}
      </div>
    </main>
  </body>
</html>
