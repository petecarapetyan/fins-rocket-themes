{% extends 'layout-default.njk' %}

{% block themeBody %}

  <div class="ga-head-l"></div>
  <div class="theme-able ga-head-main">
    <p>Header</p>
  </div>

  <div class="ga-head-r"></div>

  <div class="theme-able ga-head2-main">
    <p>head2-main</p>
  </div>

  <div class="theme-able ga-head3-main">
    <p>head3-main</p>
  </div>

  <div class="header-grid-area">
    {% block header %}
      {% include 'partials/header.njk' %}
    {% endblock header%}
  </div>

  <div class="theme-able ga-head5-main">
    <p>head5-main</p>
  </div>

  <div class="ga-content-l"></div>
  <div class="content-grid-area">
    {% block content %}
      {% include 'partials/_layoutSidebar/content.njk' %}
    {% endblock content %}
  </div>
  <div class="sidebar-grid-area">
    {% block sidebar %}
      {% include 'partials/_layoutSidebar/sidebar.njk' %}
    {% endblock sidebar %}
  </div>
  <div class="ga-content-r"></div>

  <div class="ga-foot-main">
    {% block footer %}
      {% include 'partials/footer.njk' %}
    {% endblock footer %}
  </div>

  {% include 'partials/bottom.njk' %}
{% endblock themeBody %}
