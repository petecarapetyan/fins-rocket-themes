<!doctype html>
  {% block htmlTag %}
    <html lang="{{site.lang}}" dir="{{site.dir}}">
  {% endblock htmlTag %}
  <head>
    {% block headContent %}
      {% include 'partials/head-content.njk' %}
    {% endblock %}
  </head>
  <body layout="home">
    <div class="theme-able ga-head-l"></div>
    <div class="theme-able ga-head-main">
      <p>Header</p>
    </div>
    <div class="theme-able ga-head-r"></div>

    <div class="theme-able ga-head2-main">
      <p>head2-main</p>
    </div>

    <div class="theme-able ga-head3-main">
      <p>head3-main</p>
    </div>

    <div class="rocket-content header-grid-area">
        {% block header %}
          {% include 'partials/header.njk' %}
        {% endblock header%}
    </div>

    <div class="theme-able ga-head5-main">
      <p>head5-main</p>
    </div>

    <div class="theme-able ga-content-l"></div>
    <div class="rocket-content content-grid-area">
      <p>content not showing, but why?</p>
        {% block main %}
          <main class="markdown-body">
            {{ content.html | safe }}
            {% include 'partials/previousNext.njk' %}
          </main>
        {% endblock main %}
    </div>
    <div class="rocket-content sidebar-grid-area">
        <p>sidebar not showing due to layout.css?</p>
        {% block sidebar %}
          <rocket-drawer id="sidebar">
            <nav slot="content" id="sidebar-nav">
              {% include 'partials/logoLink.njk' %}
              <rocket-navigation>
                <ul>
                  {%- for entry in collections.header %}
                    <li>
                      <a href="{{ entry.url | url }}" class="
                        {% if entry.url == page.url %} current {% endif %}
                        {% if (page.url.search(entry.url) !== -1) and (page.url !== '/') %} active {% endif %}
                      ">{{ entry.data.eleventyNavigation.key }}</a>
                    </li>
                  {%- endfor %}
                </ul>
                {% include 'partials/mobile-sidebar-bottom.njk' %}
              </rocket-navigation>
            </nav>
          </rocket-drawer>
        {% endblock sidebar %}
    </div>
    <div class="theme-able ga-content-r"></div>

    <div class="theme-able ga-foot-main">
        {% block footer %}
          {% include 'partials/footer.njk' %}
          {% include 'partials/pageJs.njk' %}
          {% include 'partials/mobileNavigationJs.njk' %}
        {% endblock footer %}
    </div>
  </body>


</html>