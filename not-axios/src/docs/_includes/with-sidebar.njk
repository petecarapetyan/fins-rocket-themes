<!doctype html>
  {% block htmlTag %}
    <html lang="{{site.lang}}" dir="{{site.dir}}">
  {% endblock htmlTag %}
  <head>
    {% block headContent %}
      {% include 'partials/head-content.njk' %}
    {% endblock %}
  </head>


  <body layout="sidebar">
    <aside id="sidenav-open">
      {% block sidebarOverlay %}
        <nav id="sidebar-nav">
          <a class="logo-link" href="{{ '/' | url }}">
            <span>{{ site.name }}</span>
          </a>
          <rocket-navigation>
            <ul>
              {%- for entry in collections.header %}
                <li>
                  <a href="{{ entry.url | url }}" class="
                    {% if entry.url == page.url %} current {% endif %}
                    {% if (page.url.search(entry.url) !== -1) and (page.url !== '/') %} active {% endif %}
                  ">{{ entry.data.eleventyNavigation.key }}</a>
                </li>
              {%- endfor %}
            </ul>
            {% include 'partials/mobile-sidebar-bottom.njk' %}
          </rocket-navigation>
        </nav>
      {% endblock sidebarOverlay %}

      <a
        href="#"
        id="sidenav-close"
        title="Close Menu"
        aria-label="Close Menu"
        onclick="window.history.back()"
      ></a>
    </aside>

    <main>
      <div class="ga-head-l"></div>
      <div class="rocket-content header-grid-area">
        <a
          href="#sidenav-open"
          id="sidenav-button"
          class="hamburger"
          title="Open Menu"
          aria-label="Open Menu"
        >
          <svg
            xmlns="http://www.w3.org/2000/svg"
            width="24"
            height="24"
            viewBox="0 0 24 24"
            fill="none"
            stroke="currentColor"
            stroke-width="2"
            stroke-linecap="round"
            stroke-linejoin="round"
            class="feather feather-menu"
          >
            <line x1="3" y1="12" x2="21" y2="12"></line>
            <line x1="3" y1="6" x2="21" y2="6"></line>
            <line x1="3" y1="18" x2="21" y2="18"></line>
          </svg>
        </a>
        <p>Rocket Header</p>
      </div>
      <div class="ga-head-r"></div>

      <div class="ga-content-l"></div>
      <div class="rocket-content content-grid-area">
        {% block main %}
          <main class="markdown-body">
            {{ content.html | safe }}
            {% include 'partials/previousNext.njk' %}
          </main>
        {% endblock main %}
      </div>
      <div class="rocket-content sidebar-grid-area">
        {% block sidebar %}
          <nav id="sidebar-nav">
            <a class="logo-link" href="{{ '/' | url }}">
              <span>{{ site.name }}</span>
            </a>
            <rocket-navigation>
              <ul>
                {%- for entry in collections.header %}
                  <li>
                    <a href="{{ entry.url | url }}" class="
                      {% if entry.url == page.url %} current {% endif %}
                      {% if (page.url.search(entry.url) !== -1) and (page.url !== '/') %} active {% endif %}
                    ">{{ entry.data.eleventyNavigation.key }}</a>
                  </li>
                {%- endfor %}
              </ul>
              {% include 'partials/mobile-sidebar-bottom.njk' %}
            </rocket-navigation>
          </nav>
        {% endblock sidebar %}
      </div>
      <div class="ga-content-r"></div>

      <div class="ga-foot-main">
        <p>Footer</p>
      </div>
    </main>
  </body>
</html>
