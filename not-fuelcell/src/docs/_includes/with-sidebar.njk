<!doctype html>
  {% block htmlTag %}
    <html lang="{{site.lang}}" dir="{{site.dir}}">
  {% endblock htmlTag %}
  <head>
    {% block headContent %}
      {% include 'partials/head-content.njk' %}
    {% endblock %}
  </head>


  <body layout="sidebar">
    <aside id="sidenav-open">
      {% block sidebarOverlay %}
        <nav slot="content" id="sidebar-nav">
          {% include 'partials/logoLink.njk' %}
          <rocket-navigation>
            <ul>
              {%- for entry in collections.header %}
                <li>
                  <a href="{{ entry.url | url }}" class="
                    {% if entry.url == page.url %} current {% endif %}
                    {% if (page.url.search(entry.url) !== -1) and (page.url !== '/') %} active {% endif %}
                  ">{{ entry.data.eleventyNavigation.key }}</a>
                </li>
              {%- endfor %}
            </ul>
            {% include 'partials/mobile-sidebar-bottom.njk' %}
          </rocket-navigation>
        </nav>
      {% endblock sidebarOverlay %}
      <a
        href="#"
        id="sidenav-close"
        title="Close Menu"
        aria-label="Close Menu"
        onclick="window.history.back()"
      ></a>
    </aside>

    <main>
      <div class="header-grid-area">
        {% block header %}
          {% include 'partials/header.njk' %}
        {% endblock header%}
      </div>
      <div class="rocket-content ga-head2-main">
        <p>Advertising Banner Here</p>
      </div>
      <div class="ga-content-l"></div>
      <div class="rocket-content ga-head3-main">
        <p>Some Header Here</p>
      </div>
      <div class="rocket-content content-grid-area">
        {% block main %}
          <div class="markdown-body">
            {{ content.html | safe }}
            {% include 'partials/previousNext.njk' %}
          </div>
        {% endblock main %}
      </div>
      <div class="sidebar-grid-area">
        {% block sidebar %}
            <nav slot="content" id="sidebar-nav">
              {% include 'partials/logoLink.njk' %}
              <rocket-navigation>
                <ul>
                  {%- for entry in collections.header %}
                    <li>
                      <a href="{{ entry.url | url }}" class="
                        {% if entry.url == page.url %} current {% endif %}
                        {% if (page.url.search(entry.url) !== -1) and (page.url !== '/') %} active {% endif %}
                      ">{{ entry.data.eleventyNavigation.key }}</a>
                    </li>
                  {%- endfor %}
                </ul>
                {% include 'partials/mobile-sidebar-bottom.njk' %}
              </rocket-navigation>
            </nav>
        {% endblock sidebar %}
      </div>
      <div class="ga-content-r"></div>

      <div class="ga-foot-main">
        {% block footer %}
          {% include 'partials/footer.njk' %}
          {% include 'partials/pageJs.njk' %}
          {% include 'partials/mobileNavigationJs.njk' %}
        {% endblock footer %}
      </div>
    </main>
  </body>
</html>
